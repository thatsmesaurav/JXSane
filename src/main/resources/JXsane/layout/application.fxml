<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ComboBox?>

<BorderPane id="app" stylesheets="@../application.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="sane.JXSane.controller.ApplicationController">
	<top>
		<HBox id="topBox">
			<padding>
				<Insets bottom="30.0" left="15.0" right="15.0" top="30.0" />
			</padding>
			<Button text="top" />
			<ComboBox fx:id="availableScanners" layoutX="15.0" layoutY="33.0" prefWidth="90.0" promptText="choose"/>
	    </HBox>
	</top>
	<left>
		<VBox id="leftBox" />
	</left>
        <right>
		<VBox id="rightBox" />
	</right>
	<center>
		<ImageView />
	</center>
	<bottom>
		<HBox id="bottomBox" />
	</bottom>
</BorderPane>
